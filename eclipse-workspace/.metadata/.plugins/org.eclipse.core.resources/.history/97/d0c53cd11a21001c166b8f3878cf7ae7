package programme.reverse;
/*OUTPUT-1
 * 1234 
 * 567 
 * 89
 * 10
 */
public class Forloop {

	public static void main(String[] args) {
		// for(initialization;condition;incriment)
		int k = 1;
		for (int i = 0; i <= 4; i++) {
			for (int j = 1; j <= 4 - i; j++) {
				System.out.print(k);
				System.out.print("\t");
				k++;
			}
           System.out.println("");
		}

	}

}

/*OUTPUT-2
 * 1 
 * 23
 * 456
 * 7890
 */

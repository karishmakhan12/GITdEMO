package programme;


 public class factorial{  
			 public static void main(String args[]){  
			  int i,x=1;  
			  int number=5;//It is the number to calculate factorial    
			  for(i=1;i<=number;i++){    
			      x=x*i;    
			  }    
			  System.out.println("Factorial of "+number+" is: "+x);    
			 }  
			}  
	



package programme.palendrom;

public class Palendrom {
	public void checkpalen() {
		int r;
		int num = 0;
		int x;
		int n = 1331;

		x = n;
		while (n > 0) {
			r = n % 10;
			num = (num * 10) + r;
			n = n / 10;
		}
		if (x == num)
			System.out.println("palindrome number ");
		else
			System.out.println("not palindrome");
	}
}

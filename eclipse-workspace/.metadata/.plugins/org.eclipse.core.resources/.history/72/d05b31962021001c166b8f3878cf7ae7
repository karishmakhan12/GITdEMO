package programme.forloop;

public class ForLoop4 {

	public static void main(String[] args) {
		/*
		 * OUTPUT 
		 *                                              QUESTION 4
		 *  3
		 *   6 9
		 *   12 15 18
		 */
        int k =3;
		for(int i=1;i<=3;i++) {
			for(int j=i;j<=i;j++) {
				System.out.print(k);
				
			}
			
		}
	}

}

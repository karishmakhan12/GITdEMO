package programme.factorial;
public class factorial{
 public static void main(String[] args)  {
		factorial fact  = new factorial();
		fact.facto();
	}
 private void facto() {
	 		  int i,x=1;  
		  int number=3;
		  for(i=1;i<=number;i++){    
		      x=x*1; 
		  }      
		  System.out.println("Factorial of "+number+" is: "+x);  
 
 }

}
package programme.palendrom;

public class Palendrom {
	public boolean checkpalen(int a, int num,int t) {
        t = num;
		while (a > 0) {
			int r;
			r = a % 10;
			num = (num * 10) + r;
			a = a / 10;
			
		}

		if (t==num) {
		return true;
		}
		else {
			System.out.println("is not palendrom");
		}
	}

	
}
